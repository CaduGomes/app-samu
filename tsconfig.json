{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@data/*": ["./src/data/*", "./src/data/*/index"],
      "@domain/*": ["src/domain/*", "src/domain/*/index"],
      "@infra/*": ["src/infra/*", "src/main/*/index"],
      "@main/*": ["src/main/*", "src/main/*/index"],
      "@presentation/*": ["src/presentation/*", "src/presentation/*/index"],
    },
    "strict": true
  },
  
}
